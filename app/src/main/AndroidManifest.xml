        android:label="Money Manager"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ManHinhChiTien"></activity>
        <activity android:name=".ManHinhTaoVi" />
        <activity android:name=".ManHinhDangKy" />
        <activity android:name=".ManHinhDangNhap" />
        <activity android:name=".api" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>